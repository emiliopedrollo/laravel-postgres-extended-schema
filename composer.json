{
  "name": "emiliopedrollo/laravel-postgres-extended-schema",
  "description": "Eloquent Extended, added some PostgreSQL features",
  "keywords": ["laravel", "eloquent", "database", "postgresql", "database"],
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Mirza Pasic",
      "email": "mirza@bosnadev.com"
    },
    {
      "name": "Peter Haza",
      "email": "peter.haza@gmail.com"
    },{
      "name": "Em√≠lio B. Pedrollo",
      "email": "emiliopedrollo@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "ext-pdo": "*",
    "illuminate/database": "^12.0",
    "ramsey/uuid": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "mockery/mockery": "^1.4.2",
    "illuminate/container": "^12.0",
    "illuminate/support": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Pedrollo\\Database\\": "src/Pedrollo/Database/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pedrollo\\Database\\DatabaseServiceProvider"
      ]
    }
  }
}
